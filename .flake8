[flake8]
max-line-length = 120
extend-ignore = E203, W503, E402, E712, F811, F401, F541, F821
exclude =
    .git,
    __pycache__,
    *.pyc,
    venv,
    env,
    .venv,
    migrations,
    node_modules,
    build,
    dist
per-file-ignores =
    __init__.py: F401
    backend/__init__.py: F401
    backend/test_*.py: F401